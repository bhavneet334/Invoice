
<h3><a class="btn" href="/item"><i class="fa fa-plus"></i> Insert New Item</a> Item List</h3>
<table class="table table-hover table-striped">
   <thead>
       <tr>
           <th> S.NO </th>
           <th> Item Description </th>
           <th> Quantity </th>
           <th> Unit Price </th>
           <th> Total </th>
           <th></th>
       </tr>
   </thead>
   <tbody>
       {{#each list}}
       <tr>
           <td>{{this.Sno}}</td>
            <td>{{this.itemDescription}}</td>
            <td>{{this.unitPrice}}</td>
            <td>{{this.quantity}}</td>
            <td>{{this.totalPrice}}</td> 
       </tr>
       {{/each}}
   </tbody>
</table>